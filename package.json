{
    "name": "@fluxprotocol/amm-sdk",
    "version": "1.4.1",
    "description": "Flux SDK that's using the Flux AMM Protocol",
    "main": "dist/FluxSdk.js",
    "types": "dist/FluxSdk.d.ts",
    "scripts": {
        "test": "jest",
        "coverage": "jest --coverage",
        "build": "webpack",
        "clean": "tsc --build --clean",
        "release": "npm run build && standard-version && npm publish"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/fluxprotocol/amm-sdk.git"
    },
    "author": "Franklin Waller <fr.l.waller@gmail.com>",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/fluxprotocol/amm-sdk/issues"
    },
    "homepage": "https://github.com/fluxprotocol/amm-sdk#readme",
    "babel": {
        "plugins": [
            "@babel/plugin-proposal-class-properties"
        ],
        "presets": [
            "@babel/preset-env",
            [
                "@babel/preset-typescript",
                {
                    "allowDeclareFields": true
                }
            ]
        ]
    },
    "devDependencies": {
        "@babel/core": "^7.12.17",
        "@babel/plugin-proposal-class-properties": "^7.12.13",
        "@babel/preset-env": "^7.12.17",
        "@babel/preset-typescript": "^7.12.17",
        "@types/big.js": "^6.0.2",
        "babel-loader": "^8.2.2",
        "declaration-bundler-webpack-plugin": "^1.0.3",
        "jest": "^26.6.3",
        "remove-strict-webpack-plugin": "^0.1.2",
        "standard-version": "^9.1.0",
        "ts-jest": "^26.5.0",
        "ts-loader": "^8.0.17",
        "typescript": "^4.1.5",
        "webpack": "4.44.2",
        "webpack-cli": "^4.5.0"
    },
    "dependencies": {
        "@fvictorio/newton-raphson-method": "^1.0.5",
        "big.js": "^6.0.3",
        "date-fns": "^2.17.0",
        "near-api-js": "^0.36.3"
    }
}
